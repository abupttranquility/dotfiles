{
   "background": {
      "scripts": [ "common.js", "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "assets/images/logo-19-active.png",
         "38": "assets/images/logo-38-active.png"
      },
      "default_title": "HoverCards"
   },
   "content_scripts": [ {
      "all_frames": false,
      "css": [ "top-frame.css" ],
      "js": [ "common.js", "top-frame.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self' https://www.youtube.com",
   "default_locale": "en",
   "description": "__MSG_app_description__",
   "homepage_url": "http://hovercards.com",
   "icons": {
      "128": "assets/images/logo-128.png",
      "16": "assets/images/logo-16-active.png",
      "19": "assets/images/logo-19-active.png",
      "38": "assets/images/logo-38-active.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "45",
   "name": "__MSG_app_name__",
   "options_page": "options.html",
   "options_ui": {
      "chrome_style": true,
      "page": "options.html"
   },
   "permissions": [ "activeTab", "identity", "storage", "webRequest", "webRequestBlocking", "http://*/*", "https://*/*" ],
   "short_name": "__MSG_app_short_name__",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.10.2",
   "web_accessible_resources": [ "*.map", "assets/*" ]
}
