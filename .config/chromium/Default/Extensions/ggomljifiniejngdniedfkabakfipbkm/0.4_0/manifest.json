{
   "content_scripts": [ {
      "js": [ "jquery-2.1.3.min.js", "4c.js" ],
      "matches": [ "http://www.4chan.org/*", "https://www.4chan.org/*", "http://boards.4chan.org/*", "https://boards.4chan.org/*" ]
   } ],
   "description": "Expands all images & webm's in a thread with the click of a button. Once expanded, you can choose to save all images into a folder.",
   "icons": {
      "128": "icon128.png",
      "16": "icon16.png",
      "48": "icon48.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "4chan Image Expander & Saver",
   "permissions": [ "activeTab", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "0.4"
}
